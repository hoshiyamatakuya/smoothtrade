# 自分の投稿にいいねした場合は一覧に表示されないように
<% notifications = @notifications.where.not(visitor_id: current_user.id) %>
<div class= 'container'>
  <div class = title >
    <h2> I18n.t('notice.title') </h2>
    <% if notifications.exists? %>
      <%= render notifications %>
    <% else %>
      <p> 通知はありません </p>
    <% end %>
  </div>
</div>
